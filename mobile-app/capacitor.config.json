{
  "appId": "com.newhavenhangouts.app",
  "appName": "New Haven Hangouts",
  "webDir": "www",
  "server": {
    "androidScheme": "https",
    "iosScheme": "ionic",
    "cleartext": true
  },
  "loggingBehavior": "debug",
  "plugins": {
    "CapacitorHttp": {
      "enabled": true
    },
    "PushNotifications": {
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ]
    }
  }
}